{
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.10",
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/main.py"
      }
    ]
  }

